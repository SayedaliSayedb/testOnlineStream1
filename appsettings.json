{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Microsoft.AspNetCore.Http.Connections": "Debug"
    }
  },
  "AllowedHosts": "*",
  "AWS": {
    "BucketName": "quiz-contest-bucket",
    "Region": "us-east-1"
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "Application": {
    "Name": "WebRTC Live Quiz System",
    "Version": "1.0.0",
    "MaxHearts": 10,
    "InitialHearts": 3,
    "QuizTimeLimit": 30,
    "EnableDetailedLogging": false
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 52428800
    }
  }
}